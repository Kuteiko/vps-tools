{
	"name": "pulsarvp/vps-tools",
	"description": "Tools and other stuff that are used in every PulsarVP project.",
	"type": "library",
	"license": "MIT",
	"homepage": "http://pulsarvp.ru",
	"authors": [
		{
			"name": "Anna Manaenkova",
			"email": "anna.manaenkova@phystech.edu"
		},
		{
			"name": "Evgenii Kuteiko",
			"email": "kuteiko@mail.ru "
		}
	],
	"require": {
		"yiisoft/yii2": ">=2.0.7",
		"yiisoft/yii2-bootstrap": ">=2.0.0",
		"yiisoft/yii2-authclient": ">=2.0.0",
		"yiisoft/yii2-apidoc": ">=2.0.0",
		"creocoder/yii2-nested-sets": "*",
		"yiisoft/yii2-smarty": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "*",
		"phpunit/dbunit": "*",
		"phpdocumentor/phpdocumentor": "*"
	},
	"autoload": {
		"psr-4": {
			"vps\\tools\\": "src"
		}
	},
	"extra": {
		"asset-installer-paths": {
			"npm-asset-library": "vendor/npm",
			"bower-asset-library": "vendor/bower"
		}
	}
}
